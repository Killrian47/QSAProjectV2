{% if entreprisesStartByC is defined %}
    {% if entreprisesStartByC is not empty %}
        <div>
            <h2>C</h2>
            <hr class="sep w-50">
            <div class="list-entreprise">
                {% for entreprise in entreprisesStartByC %}
                    {% include 'admin/entreprise/entreprise.html.twig' %}
                {% endfor %}
            </div>
        </div>
    {% else %}
        <div>
            <h2>C</h2>
            <hr class="sep w-50">
            <div class="d-flex justify-content-center align-items-center">
                <p>Pas d'entreprise commencant par C</p>
            </div>
        </div>
    {% endif %}
{% endif %}