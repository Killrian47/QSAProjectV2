{% if entreprisesStartByB is defined %}
    {% if entreprisesStartByB is not empty %}
        <div>
            <h2>B</h2>
            <hr class="sep w-50">
            <div class="list-entreprise">
                {% for entreprise in entreprisesStartByB %}
                    {% include 'admin/entreprise/entreprise.html.twig' %}
                {% endfor %}
            </div>
        </div>
    {% else %}
        <div>
            <h2>B</h2>
            <hr class="sep w-50">
            <div class="d-flex justify-content-center align-items-center">
                <p>Pas d'entreprise commencant par B</p>
            </div>
        </div>
    {% endif %}
{% endif %}